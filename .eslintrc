// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "prefer-arrow-callback": 0,
    "vars-on-top": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "block-spacing": [2, "always"],
    "prefer-template": 0,
    "func-names": 0,
    "prefer-rest-params": 0,
    "no-param-reassign": 0
  }
}
